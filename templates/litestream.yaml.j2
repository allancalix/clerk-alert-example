access-key-id: {{ S3_ACCESS_KEY_ID }}
secret-access-key: {{ S3_SECRET_KEY }}

dbs:
  - path: "{{ PWD }}/.clerk/data/clerk.db"
    replicas:
      - type: s3
        bucket: {{ S3_BUCKET }}
        path: clerk-alert-example/clerk.db # Update to whatever path you'd like
        endpoint: {{ S3_METADATA_URL }}
        force-path-style: true
